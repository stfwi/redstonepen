{
  "schemaVersion": 1,
  "id": "redstonepen",
  "version": "${version}",
  "name": "Redstone Pen",
  "description": "Adds a pen to draw detailed Redstone tracks, as well as Redstone relays, and the Redstone Logic Controller.",
  "authors": [ "wilechaote" ],
  "contact": {
    "homepage": "https://github.com/stfwi/redstonepen",
    "sources": "https://github.com/stfwi/redstonepen"
  },
  "license": "MIT",
  "icon": "assets/redstonepen/logo.png",
  "environment": "*",
  "entrypoints": {
    "main": [
      "wile.redstonepen.ModRedstonePen"
    ],
    "client": [
      "wile.redstonepen.ModRedstonePenClient"
    ]
  },
  "mixins": [
    "redstonepen.mixins.json"
  ],
  "depends": {
    "fabricloader": ">=0.14.6",
    "fabric": "*",
    "minecraft": "~1.20.1",
    "java": ">=17"
  },
  "suggests": {
    "another-mod": "*"
  }
}